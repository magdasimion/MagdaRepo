
@{
    ViewBag.Title = "Admin";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <div>

        <ul>
            <li>
                @ViewBag.a
            </li>
            <li>
                @ViewBag.b
            </li>

            <li>
                @ViewBag.c
            </li>
            <li>
                @ViewBag.d
            </li>
            <li>
                @ViewBag.e
            </li>
        </ul>

    </div>
    <div>

        <ul>
            <li>
                @ViewBag.f
            </li>

        </ul>
        <ul>
            <li>
                @ViewBag.g
            </li>

        </ul>
        <ul>
            <li>
                @ViewBag.h
            </li>

        </ul>

    </div>
    @{var price = 20;}
    <div>

        @if (price > 30)
        {
            <p>The price is too high.</p>
        }
        else
        {
            <p>The price is OK.</p>
        }

    </div>

    <div class="btn-group">
        <button type="button" class="btn btn-primary"> <a href="https://en.wikipedia.org/wiki/Tablet_computer">Apple</a></button>
        <button type="button" class="btn btn-primary"><a href="https://en.wikipedia.org/wiki/Tablet_computer">Samsung</a></button>
        <button type="button" class="btn btn-primary"><a href="https://en.wikipedia.org/wiki/Tablet_computer"><b>Sony</b></a></button>



    </div>

    <div class="btn-group">
        <button type="button" class="btn btn-primary">Sony</button>
        <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
            <span class="caret"></span>
        </button>
        <ul class="dropdown-menu" role="menu">
            <li><a href="https://en.wikipedia.org/wiki/Tablet_computer">Tablet</a></li>
            <li><a href="https://en.wikipedia.org/wiki/Smartphone">Smartphone</a></li>
        </ul>
    </div>

</body>
</html>
